{
	"$schema": "https://turbo.build/schema.json",
	"globalDependencies": ["**/.env.*local"],
	"pipeline": {
		"db:push": {
			"cache": false
		},
		"lint": {},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"build": {
			"dependsOn": ["^build", "^db:generate"],
			"outputs": [".next/**", "!.next/cache/**"]
		},
		"db:generate": {
			"cache": false
		}
	}
}
